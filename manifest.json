{
  "name": "GIWT - Grab It With Tentacle",
  "version": "1.0",
  "description": "Add blogs to your Tentacle feed in one click",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_title": "Grab It With Tentacle"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}